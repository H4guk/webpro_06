<!Doctype html>
<html>
<head>
  <meta charset="utf-8">
  <title>fgo 素材一覧</title>
</head>
<body>
  <table>
    <h2>fgo 素材一覧</h2>
    <tr><th>ID</th><th>素材名</th><tr>
      <% let number = 0 %>
      <% for( let row of data ) { %>
        <tr><td><%= row.id %></td>
        <td><a href="/fgo/<%= row.id %>"><%= row.name %></a></td></tr>
        <% number += 1 %>
      <% } %>
  </table>
  <a href="/public/fgo_add.html">素材を追加</a>
</body>
</html>